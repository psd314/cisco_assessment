#!/home/philippe/Desktop/exercises/ex_env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Exercises','console_scripts','exercises'
__requires__ = 'Exercises'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Exercises', 'console_scripts', 'exercises')()
    )
